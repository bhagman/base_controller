#include <Wire.h>

void setup()
{
  Wire.begin();
  Serial.beigin(9600);
}


void loop()
{
  Wire.requestFrom(1,3);
  
  Serial.print("Position: ");
  while(Wire.available())
  {
    char c = Wire.read();
    Serial.print(c);
  }
  Serial.println(" ");
  
  if(Serial.availble() > 0)
  {
    int new_set_point = Serial.read();
    Wire.beginTransmission(1);
    Wire.write(new_set_point);
    Wire.endTransmission();
  }
    
}